<schemalist>
  <schema id="org.berry.maynard"
          path="/org/berry/maynard/"
          gettext-domain="@GETTEXT_PACKAGE@">

    <key name="favorites" type="as">
      <default>[ 'org.gnome.gedit.desktop', 'lxterminal.desktop', 'pcmanfm.desktop', 'thunderbird.desktop', 'firefox.desktop' ]</default>
      <_summary>List of desktop file IDs for favorite applications</_summary>
      <_description>
        The applications corresponding to these identifiers
        will be displayed in the panel.
      </_description>
    </key>

  </schema>
</schemalist>
